{
    "name": "radiograb",
    "version": "0.0.1",
    "description": "Radio recorder",
    "license": "MIT",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "electron-packager --overwrite --no-prune --out dist --platform=linux,win32,darwin .",
        "build-linux": "electron-packager --overwrite --no-prune --out dist --platform=linux .",
        "build-win": "electron-packager --overwrite --no-prune --out dist --platform=win32 .",
        "build-mac": "electron-packager --overwrite --no-prune --out dist --platform=darwin ."
    },
    "dependencies": {
        "child-process-promise": "^2.2.1",
        "fs-extra": "^4.0.2",
        "ionicons": "^2.0.0",
        "json-path": "^0.1.3",
        "lodash": "^4.17.4",
        "mkdirp-promise": "^5.0.1",
        "moment": "^2.19.1",
        "mp3-duration": "^1.1.0",
        "open": "^0.0.5",
        "request": "^2.83.0",
        "typeface-dosis": "^0.0.44",
        "typeface-rajdhani": "^0.0.44",
        "vue": "^2.5.2"
    },
    "devDependencies": {
        "babel-eslint": "^8.0.1",
        "electron": "^1.7.9",
        "electron-packager": "^9.1.0"
    }
}
